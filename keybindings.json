[
    { "key": "ctrl+alt+left", "command": "workbench.action.focusPreviousGroup" },
    { "key": "ctrl+alt+right", "command": "workbench.action.focusNextGroup" },
    { "key": "ctrl+k ctrl+enter", "command": "workbench.action.keepEditor" },
    { "key": "cmd+k cmd+enter", "command": "workbench.action.keepEditor" },

    { "key": "ctrl+k ctrl+b", "command": "workbench.action.tasks.terminate" },

    { "key": "ctrl+alt+d", "command": "workbench.action.git.globalOpenChange",
        "when": "editorTextFocus" },
    { "key": "ctrl+alt+d", "command": "workbench.action.git.globalOpenFile",
        "when": "textCompareEditorVisible" },

    { "key": "cmd+y", "command": "redo",
        "when": "editorTextFocus && !editorReadonly" },

    { "key": "cmd+0", "command": "workbench.action.zoomReset" },

    { "key": "cmd+shift+g", "command": "workbench.view.git" },

    { "key": "f8", "command": "workbench.action.debug.continue",
        "when": "inDebugMode" },
    { "key": "f5", "command": "-workbench.action.debug.continue",
        "when": "inDebugMode" },
    { "key": "f5", "command": "workbench.action.debug.start" },

    { "key": "f7", "command": "editor.action.marker.next",
        "when": "editorFocus && !editorReadonly" },
    { "key": "shift+f7", "command": "editor.action.marker.prev",
        "when": "editorFocus && !editorReadonly" },

    { "key": "ctrl+`", "command": "workbench.action.terminal.focus",
        "when": "!terminalFocus" },
    { "key": "ctrl+`", "command": "workbench.action.focusActiveEditorGroup",
        "when": "terminalFocus" },

    { "key": "cmd+r", "command": "workbench.action.openRecent" },

    { "key": "cmd+f9", "command": "editor.debug.action.conditionalBreakpoint" },

    {
        "key": "ctrl+down",
        "command": "workbench.action.compareEditor.nextChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "ctrl+up",
        "command": "workbench.action.compareEditor.previousChange",
        "when": "textCompareEditorVisible"
    },

    { "key": "shift+cmd+]",           "command": "-spotify.next" },
    { "key": "shift+cmd+[",           "command": "-spotify.previous" },

    {
        "key": "shift+cmd+i",
        "command": "workbench.action.toggleDevTools",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+alt+i",
        "command": "workbench.action.toggleDevTools",
        "when": "editorTextFocus"
    },

    {
        "key": "cmd+m",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "ctrl+shift+cmd+t",
        "command": "esQuotes.transformToTemplateString",
        "when": "editorTextFocus"
    },

    {
        "key": "ctrl+alt+left",
        "command": "subwordNavigation.cursorSubwordLeft",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+right",
        "command": "subwordNavigation.cursorSubwordRight",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+left",
        "command": "subwordNavigation.cursorSubwordLeftSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+right",
        "command": "subwordNavigation.cursorSubwordRightSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+backspace",
        "command": "subwordNavigation.deleteSubwordLeft",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+delete",
        "command": "subwordNavigation.deleteSubwordRight",
        "when": "editorTextFocus"
    },

    {
        "key": "cmd+shift+j",
        "command": "workbench.action.focusPanel",
        "when": "editorTextFocus"
    },

    { "key": "cmd+p", "command": "workbench.action.quickOpenNavigateNext",
        "when": "inQuickOpen" },
    { "key": "cmd+shift+p", "command": "workbench.action.quickOpenNavigatePrevious",
        "when": "inQuickOpen" },

    {
        "key": "ctrl+\\",
        "command": "editor.action.insertSnippet",
        "when": ""
    },

    {
        "key": "cmd+;",
        "command": "cursorRight",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+cmd+;",
        "command": "cursorLeft",
        "when": "editorTextFocus"
    },

    {
        "key": "ctrl+f12",
        "command": "editor.action.goToTypeDefinition",
        "when": "editorTextFocus"
    },

    {
        "key": "cmd+shift+y",
        "command": "workbench.action.output.toggleOutput"
    },
    {
        "key": "cmd+shift+u",
        "command": "workbench.debug.action.toggleRepl"
    },

    {
        "key": "ctrl+shift+x",
        "command": "workbench.action.terminal.kill"
    },
    {
        "key": "ctrl+shift+j",
        "command": "workbench.action.terminal.focusNext"
    },
    {
        "key": "ctrl+shift+k",
        "command": "workbench.action.terminal.focusPrevious"
    },
    {
        "key": "ctrl+shift+x",
        "command": "workbench.action.terminal.kill"
    },
    {
        "key": "ctrl+shift+n",
        "command": "workbench.action.terminal.new",
        "when": "terminalFocus"
    },

    {
        "key": "cmd+8",
        "command": "editor.action.showHover",
        "when": "editorTextFocus"
    },

    {
        "key": "cmd+k cmd+m",
        "command": "workbench.action.editor.changeLanguageMode",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+8",
        "command": "editor.debug.action.showDebugHover",
        "when": "editorTextFocus && inDebugMode"
    },
    {
        "key": "ctrl+shift+cmd+d",
        "command": "workbench.debug.viewlet.action.toggleBreakpointsActivatedAction"
    },
    {
        "key": "shift+cmd+s",
        "command": "git.stage"
    }
]