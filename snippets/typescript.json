{
	 "import": {
		"prefix": "impr",
		"body": [
			"import {$2} from '$1';"
		],
		"description": "import"
	},
	"fn call": {
		"prefix": "fn",
		"body": [
			"($1)$2"
		],
		"description": "()"
	},
	"log": {
		"prefix": "log",
		"body": "console.log(`$1`);",
		"description": "console.log"
	},
	"log value": {
		"prefix": "lvl",
		"body": "console.log(`$1: \\${$1}`);",
		"description": "log the selected value"
	},
	"timed log": {
		"prefix": "time",
		"body": "console.log(`\\${Date.now()/1000}: $1`);",
		"description": "console.log + the time"
	}
}